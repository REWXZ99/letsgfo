<%- include('../layouts/main', { 
    title: 'Project Details',
    headContent: `
        <style>
            .project-detail-section {
                padding: 2rem 1rem;
            }
            
            .project-detail-container {
                max-width: 1000px;
                margin: 0 auto;
            }
            
            .project-back-link {
