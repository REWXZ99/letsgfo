            .related-project-card {
                background: rgba(10, 10, 10, 0.8);
                border: 1px solid rgba(255, 0, 0, 0.1);
                border-radius: var(--radius-xl);
                padding: 1.5rem;
                transition: all 0.3s ease;
                text-decoration: none;
                color: inherit;
                display: block;
            }
            
